                <ITEXT CH="       Таблица 3"/>
                <para/>
                <ITEXT CH="                     Режимы работы таймера"/>
                <para/>
                <ITEXT CH="       ┌───────────────────────────┬──────┬─────┬──────┐"/>
                <para/>
                <ITEXT CH="       │ Режим работы              │ BDIR │ BC1 │ BC2  │"/>
                <para/>
                <ITEXT CH="       ├───────────────────────────┼──────┼─────┼──────┤"/>
                <para/>
                <ITEXT CH="       │ Микросхема неактивна      │   0  │  0  │  1   │"/>
                <para/>
                <ITEXT CH="       │ Чтение данных из регистра │   0  │  1  │  1   │"/>
                <para/>
                <ITEXT CH="       │ Запись данных в регистр   │   1  │  0  │  1   │"/>
                <para/>
                <ITEXT CH="       │ Выбор регистра            │   1  │  1  │  1   │"/>
                <para/>
                <ITEXT CH="       └───────────────────────────┴──────┴─────┴──────┘"/>
                <para/>
                <para/>
                <ITEXT CH="          Схема дешифрации адресов портов таймера при"/>
                <para/>
                <ITEXT CH="                   подключении к &quot;Синклеру&quot;"/>
                <para/>
                <para/>
                <ITEXT CH=" __                                  8 ┌────┐"/>
                <para/>
